<h1>{{course.course_name}}</h1>


  <ul>
    <li>{{course.city}}</li>
    <li>{{course.state}}</li>
    <li>{{course.par}}</li>
    <li>{{course.myScore}}</li>
    <li>{{course.ball}}</li>
    <li>{{course.comments}}</li>
    <li>{{course.datePlayed}}</li>

  </ul>


<a href="/users/{{user.id}}/courses/{{course.id}}/edit" class="btn btn-default">Edit Course</a>
    <form method="POST" action="/users/{{user._id}}/courses/{{course._id}}?_method=DELETE">
          <button type="submit" value="DELETE" class="btn btn-danger" />Delete</button>
    </form>
