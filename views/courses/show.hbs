<h1>{{course.courseName}}</h1>


  <ul class="round">
    <li>City: {{course.city}}</li>
    <li>State: {{course.state}}</li>
    <li>Course Par: {{course.par}}</li>
    <li>My Score: {{course.myScore}}</li>
    <li>Ball Used: {{course.ball}}</li>
    <li>Comments: {{course.comments}}</li>
    <br>
    <li>Date Played:{{course.datePlayed}}</li>

  </ul>


<a href="/users/{{user.id}}/courses/{{course.id}}/edit" class="btn btn-default">Edit Course</a>
    <form method="POST" action="/users/{{user._id}}/courses/{{course._id}}?_method=DELETE">
          <button type="submit" value="DELETE" class="btn btn-danger" />Delete</button>
    </form>
<br>
<a href="/users/{{user.id}}/courses" class="btn btn-default">My Courses</a>

