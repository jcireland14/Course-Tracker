{{#if currentUser}}
  <h4>Welcome {{currentUser.email}}</h4>
  <a href="/users/{{currentUser._id}}">My Profile</a>
{{/if}}

{{#each users}}
  <ul>
    <li>{{id}}</li>
    <li>{{email}}</li>
    <li>{{password_digest}}</li>
    {{/each}}
  </ul>

<button><a href="/users/{{user.id}}/course" class="btn btn-default">Create a New Course</a></button>
